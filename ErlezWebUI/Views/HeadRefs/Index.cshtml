@model IEnumerable<ErlezWebUI.Models.HeadRef>

@{
    ViewBag.Title = "Index";
}

<h2>HeadRefs</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PostId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HeadRefQual)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HeadRef1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HeadRefDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Head.IAID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PostId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HeadRefQual)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HeadRef1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HeadRefDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Head.IAID)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ })
        </td>
    </tr>
}

</table>

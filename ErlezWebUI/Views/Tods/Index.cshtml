@model IEnumerable<ErlezWebUI.Models.Tod>

@{
    ViewBag.Title = "Index";
}

<h2>Tods</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PostId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TodTerms)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TodDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TodTermsLoc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Head.IAID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PostId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TodTerms)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TodDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TodTermsLoc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Head.IAID)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ })
        </td>
    </tr>
}

</table>
